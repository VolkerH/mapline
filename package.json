{
  "name": "mapline",
  "version": "0.1.0",
  "description": "Create a pdf of maps along a track in printable quality",
  "main": "src/index.js",
  "scripts": {
    "start": "budo src/index.js:dist/bundle.js --live -t babelify",
    "build": "browserify src/index.js | uglifyjs - -cm > dist/bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "keywords": [],
  "author": "sgelb",
  "license": "ISC",
  "dependencies": {
    "@mapbox/togeojson": "^0.16.0",
    "@turf/bbox": "^3.7.5",
    "cheap-ruler": "^2.4.1",
    "file-saver": "^1.3.3",
    "jspdf": "^1.3.2",
    "mapbox-gl": "^0.32.1",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^9.4.5",
    "uglify-js": "^2.7.5"
  }
}
